<UserControl x:Class="MemoryReadingTest.Charts.PlayTimeChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MemoryReadingTest.Charts"
             xmlns:wpf="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance local:PlayTimeChartVm,IsDesignTimeCreatable=False}">
    <wpf:CartesianChart
        Series="{Binding Series}"   ZoomMode="X"
        XAxes="{Binding XAxes}">
    </wpf:CartesianChart>
</UserControl>
